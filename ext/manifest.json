{
    "name": "Broker",
    "description": "Insurance Broker Forms Autofill",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {},
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}